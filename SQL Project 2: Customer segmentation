-- STRUCTURE OF THE TABLE 
SELECT *
FROM users
LIMIT 20;

-- BORN IN THE 80S
SELECT email, birthday
FROM users
WHERE birthday > '1980-01-01'
  AND birthday < '1989-12-31';

-- USERS SIGNED UP PRIOR TO MAY 2017
SELECT email, created_at
FROM users
WHERE created_at < '2017-05-01';

-- EMAILS OF THOSE WHO RECEIVED 'BEARS' TEST
SELECT email 
FROM users
WHERE test = 'bears';

-- ALL EMAILS WHO RECEIVED CAMPAIGN ON WEBSITE BBB
SELECT email
FROM users
WHERE campaign LIKE 'BBB%';

-- EMAILS WHO RECEIVED THE 2ND AD
SELECT email
FROM users
WHERE campaign LIKE '%2';

-- CAMPAIGN AND TEST USERS
SELECT email
FROM users 
WHERE campaign IS NOT NULL 
  AND test IS NOT NULL;
